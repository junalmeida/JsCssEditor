<?xml version="1.0" encoding="UTF-8"?>
<AddIn 
        name="Javascript and Style Sheet Editor" 
        author="Marcos Almeida Jr." 
        copyright="MIT" 
        description="Outlining, code completion, syntax coloring." 
        addInManagerHidden="preinstalled" 
        id="JsCssEditor" 
        namespace="MonoDevelop" 
        url="http://junalmeida.github.com/JsCssEditor/" 
        category="IDE extensions" 
        version="2.8.6.5">
 <Runtime>
     <Import assembly="JsCssEditor.dll" />
 </Runtime>
    
<Dependencies>
     <Addin id="Core" version="2.8.6.5"/>
     <Addin id="Ide" version="2.8.6.5"/>
     <Addin id="DesignerSupport" version="2.8.6.5"/>
     <Addin id="SourceEditor2" version="2.8.6.5"/>
 </Dependencies>
    
 <Extension path="/MonoDevelop/ProjectModel/DomParser">
     <Parser class="JsCssEditor.JavascriptDocumentParser" id="JavascriptDocumentParser" fileExtensions=".js" />
     <Parser class="JsCssEditor.CssDocumentParser" id="CssDocumentParser" fileExtensions=".css" />
 </Extension>
</AddIn>